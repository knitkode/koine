{
  "root": "packages/next",
  "sourceRoot": "packages/next",
  "projectType": "library",
  "tags": [],
  "targets": {
    "build-cjs": {
      "executor": "@nrwl/web:rollup",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/next",
        "tsConfig": "packages/next/tsconfig.lib.json",
        "project": "packages/next/package.json",
        "entryFile": "packages/next/index.ts",
        "external": ["react/jsx-runtime"],
        "format": ["umd"],
        "deleteOutputPath": false
      }
    },
    "build-tsc": {
      "executor": "@nrwl/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/.tsc/next",
        "tsConfig": "packages/next/tsconfig.lib.json",
        "project": "packages/next/package.json",
        "main": "packages/next/index.ts",
        "buildableProjectDepsInPackageJsonType": "dependencies",
        "assets": ["packages/next/*.md", "packages/next/*.d.ts"]
      }
    },
    "build-tsup": {
      "executor": "./tools/executors/tsup:build",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/next",
        "tsConfig": "packages/next/tsconfig.lib.json",
        "packageJson": "packages/next/package.json",
        "main": "packages/next/**/*.ts*",
        "assets": ["packages/next/*.md", "packages/next/*.d.ts"]
      }
    },
    "build": {
      "executor": "./tools/executors/build:build"
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/next/**/*.{ts,tsx,js,jsx}"]
      }
    }
  }
}
