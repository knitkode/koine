{
  "root": "packages/next",
  "sourceRoot": "packages/next/src",
  "projectType": "library",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nrwl/web:rollup",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/packages/next",
        "buildableProjectDepsInPackageJsonType": "dependencies",
        "tsConfig": "packages/next/tsconfig.lib.json",
        "project": "packages/next/package.json",
        "entryFile": "packages/next/src/index.ts",
        "external": ["react/jsx-runtime"],
        "rollupConfig": "packages/next/rollup.config.js",
        "format": ["esm", "umd", "cjs"],
        "compiler": "babel",
        "assets": [
          {
            "glob": "packages/next/README.md",
            "input": ".",
            "output": "."
          },
          {
            "glob": "packages/next/src/*.d.ts",
            "input": ".",
            "output": "."
          }
        ]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/next/**/*.{ts,tsx,js,jsx}"]
      }
    }
  }
}
