{
  "root": "packages/utils",
  "sourceRoot": "packages/utils/src",
  "projectType": "library",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nrwl/web:rollup",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/packages/utils",
        "buildableProjectDepsInPackageJsonType": "dependencies",
        "tsConfig": "packages/utils/tsconfig.lib.json",
        "project": "packages/utils/package.json",
        "entryFile": "packages/utils/src/index.ts",
        "external": [],
        "compiler": "babel",
        "assets": [
          {
            "glob": "packages/utils/README.md",
            "input": ".",
            "output": "."
          },
          {
            "glob": "packages/utils/src/*.d.ts",
            "input": ".",
            "output": "."
          }
        ]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/utils/**/*.ts"]
      }
    }
  }
}
