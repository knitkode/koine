{
  "root": "packages/utils",
  "sourceRoot": "packages/utils",
  "projectType": "library",
  "tags": [],
  "targets": {
    "build-cjs": {
      "executor": "@nrwl/web:rollup",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/utils",
        "tsConfig": "packages/utils/tsconfig.lib.json",
        "project": "packages/utils/package.json",
        "entryFile": "packages/utils/index.ts",
        "format": ["umd"],
        "deleteOutputPath": false
      }
    },
    "build-tsc": {
      "executor": "@nrwl/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/.tsc/utils",
        "tsConfig": "packages/utils/tsconfig.lib.json",
        "project": "packages/utils/package.json",
        "main": "packages/utils/index.ts",
        "buildableProjectDepsInPackageJsonType": "dependencies",
        "assets": ["packages/utils/*.md", "packages/utils/*.d.ts"]
      }
    },
    "build-tsup": {
      "executor": "./tools/executors/tsup:build",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/utils",
        "tsConfig": "packages/utils/tsconfig.lib.json",
        "packageJson": "packages/utils/package.json",
        "main": "packages/utils/**/*.ts*",
        "assets": ["packages/utils/*.md", "packages/utils/*.d.ts"]
      }
    },
    "build": {
      "executor": "./tools/executors/build:build"
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/utils/**/*.ts"]
      }
    },
    "version": {
      "executor": "@jscutlery/semver:version",
      "options": {
        "postTargets": ["utils:publish", "utils:github"]
      }
    },
    "github": {
      "executor": "@jscutlery/semver:github",
      "options": {
        "tag": "${tag}",
        "notes": "${notes}"
      }
    },
    "publish": {
      "executor": "ngx-deploy-npm:deploy",
      "options": {
        "access": "public"
      }
    }
  }
}
