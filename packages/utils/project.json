{
  "root": "packages/utils",
  "sourceRoot": "packages/utils",
  "projectType": "library",
  "tags": [],
  "targets": {
    "build-esm": {
      "executor": "@nrwl/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/packages/utils",
        "tsConfig": "packages/utils/tsconfig.lib.json",
        "project": "packages/utils/package.json",
        "main": "packages/utils/index.ts",
        "buildableProjectDepsInPackageJsonType": "dependencies",
        "assets": ["packages/utils/README.md", "packages/utils/typings.d.ts"]
      }
    },
    "build-web": {
      "executor": "@nrwl/web:rollup",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/packages/utils",
        "tsConfig": "packages/utils/tsconfig.lib.json",
        "project": "packages/utils/package.json",
        "entryFile": "packages/utils/index.ts",
        "format": ["umd"],
        "deleteOutputPath": false
      }
    },
    "build": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "commands": ["nx build-esm utils", "nx build-web utils"],
        "parallel": false
      }
    },
    "build-clean": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "commands": ["rm -rf dist/packages/utils/index.js"]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/utils/**/*.ts"]
      }
    }
  }
}
