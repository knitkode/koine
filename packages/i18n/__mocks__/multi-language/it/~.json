{
  "home": "/",
  "about": "/chi-siamo",
  "account": {
    "user": {
      "profile": "/account/profilo",
      "[id]": "/account/profilo/{{ id }}",
      "settings": {
        "index": "/account/profilo/impostazioni",
        "personal": "/account/profilo/impostazioni/personali"
      }
    }
  },
  "products": {
    "index": "/prodotti",
    "[id]": {
      "index": "^/{{ id }}",
      "edit": {
        "index": "^/modifica",
        "details": "^/dettagli"
      }
    }
  },
  "apps": {
    "tool": {
      "[[...spa]]": "/apps/tool",
      "things": {
        "index": "^/things",
        "[id]": {
          "index": "^/{{ id }}",
          "detail": {
            "index": "^/dettaglio",
            "new": "^/new",
            "[detailId]": {
              "index": "^/{{ detailId }}",
              "edit": "^/edit"
            }
          }
        }
      }
    }
  }
}
