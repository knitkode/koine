{
  "name": "react",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/react",
  "projectType": "library",
  "tags": [],
  "targets": {
    "build": {},
    "build-modularized": {},
    "build-bundle": {
      "options": {
        "additionalEntryPoints": [
          "packages/react/calendar.ts",
          "packages/react/forms.ts"
        ]
      }
    },
    "lint": {},
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/packages/react"],
      "options": {
        "jestConfig": "packages/react/jest.config.js",
        "passWithNoTests": true
      }
    },
    "storybook": {
      "executor": "@nx/storybook:storybook",
      "options": {
        "uiFramework": "@storybook/react",
        "port": 4400,
        "config": {
          "configFolder": "packages/react/.storybook"
        }
      },
      "configurations": {
        "ci": {
          "quiet": true
        }
      }
    },
    "storybook-build": {
      "executor": "@nx/storybook:build",
      "outputs": ["{options.outputPath}"],
      "options": {
        "uiFramework": "@storybook/react",
        "outputPath": "dist/storybook/react",
        "config": {
          "configFolder": "packages/react/.storybook"
        }
      },
      "configurations": {
        "ci": {
          "quiet": true
        }
      }
    }
  }
}
