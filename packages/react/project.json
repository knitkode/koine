{
  "root": "packages/react",
  "sourceRoot": "packages/react/src",
  "projectType": "library",
  "tags": [],
  "targets": {
    "build-esm": {
      "executor": "@nrwl/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/packages/react",
        "tsConfig": "packages/react/tsconfig.lib.json",
        "project": "packages/react/package.json",
        "main": "packages/react/index.ts",
        "buildableProjectDepsInPackageJsonType": "dependencies",
        "assets": ["packages/react/README.md", "packages/react/*.d.ts"]
      }
    },
    "build-web": {
      "executor": "@nrwl/web:rollup",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/packages/react",
        "tsConfig": "packages/react/tsconfig.lib.json",
        "project": "packages/react/package.json",
        "entryFile": "packages/react/index.ts",
        "rollupConfig": "@nrwl/react/plugins/bundle-rollup",
        "external": ["react/jsx-runtime"],
        "format": ["umd"],
        "deleteOutputPath": false
      }
    },
    "build": {
      "executor": "./tools/executors/build:build"
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/react/**/*.{ts,tsx,js,jsx}"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/packages/react"],
      "options": {
        "jestConfig": "packages/react/jest.config.js",
        "passWithNoTests": true
      }
    },
    "storybook": {
      "executor": "@nrwl/storybook:storybook",
      "options": {
        "uiFramework": "@storybook/react",
        "port": 4400,
        "config": {
          "configFolder": "packages/react/.storybook"
        }
      },
      "configurations": {
        "ci": {
          "quiet": true
        }
      }
    },
    "build-storybook": {
      "executor": "@nrwl/storybook:build",
      "outputs": ["{options.outputPath}"],
      "options": {
        "uiFramework": "@storybook/react",
        "outputPath": "dist/storybook/react",
        "config": {
          "configFolder": "packages/react/.storybook"
        }
      },
      "configurations": {
        "ci": {
          "quiet": true
        }
      }
    }
  }
}
