{
  "root": "packages/react",
  "sourceRoot": "packages/react",
  "projectType": "library",
  "tags": [],
  "targets": {
    "build-es6": {
      "executor": "./tools/executors/build:build-es6",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/react",
        "tsConfig": "packages/react/tsconfig.lib.json",
        "project": "packages/react/package.json",
        "main": "packages/react/index.ts",
        "buildableProjectDepsInPackageJsonType": "dependencies",
        "updateBuildableProjectDepsInPackageJson": true,
        "assets": ["packages/react/*.md", "packages/react/*.d.ts"]
      }
    },
    "build-cjs": {
      "executor": "./tools/executors/build:build-cjs",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/react/node",
        "tsConfig": "packages/react/tsconfig.lib.json",
        "project": "packages/react/package.json",
        "main": "packages/react/index.ts",
        "assets": []
      }
    },
    "build-umd": {
      "executor": "@nrwl/web:rollup",
      "outputs": ["{options.outputPath}"],
      "options": {
        "project": "packages/react/package.json",
        "outputPath": "dist/react",
        "entryFile": "packages/react/index.ts",
        "tsConfig": "packages/react/tsconfig.lib.json",
        "compiler": "babel",
        "umdName": "koine_react",
        "external": ["tslib"],
        "extractCss": true,
        "rollupConfig": "tools/rollup.config.js",
        "format": ["cjs", "esm", "umd"],
        "assets": ["packages/react/README.md"]
      }
    },
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          "npx nx run react:build-es6",
          "npx nx run react:build-cjs"
        ],
        "parallel": false
      }
    },
    "_build": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          "npx nx run react:build-umd",
          "npx nx run react:build-es6",
          "npx nx run react:build-cjs"
        ],
        "parallel": false
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/react/**/*.{ts,tsx,js,jsx}"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/packages/react"],
      "options": {
        "jestConfig": "packages/react/jest.config.js",
        "passWithNoTests": true
      }
    },
    "storybook": {
      "executor": "@nrwl/storybook:storybook",
      "options": {
        "uiFramework": "@storybook/react",
        "port": 4400,
        "config": {
          "configFolder": "packages/react/.storybook"
        }
      },
      "configurations": {
        "ci": {
          "quiet": true
        }
      }
    },
    "build-storybook": {
      "executor": "@nrwl/storybook:build",
      "outputs": ["{options.outputPath}"],
      "options": {
        "uiFramework": "@storybook/react",
        "outputPath": "dist/storybook/react",
        "config": {
          "configFolder": "packages/react/.storybook"
        }
      },
      "configurations": {
        "ci": {
          "quiet": true
        }
      }
    }
  }
}
