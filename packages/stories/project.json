{
  "name": "stories",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/stories/src",
  "projectType": "library",
  "tags": [],
  "targets": {
    "build": {
      "executor": "./tools/executors/build:build",
      "outputs": ["{options.outputPath}"],
      "options": {
        "format": ["cjs", "esm"],
        "outputPath": "dist/stories",
        "tsConfig": "packages/stories/tsconfig.lib.json",
        "main": "packages/stories/index.ts",
        "assets": []
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/stories/**/*.{ts,tsx,js,jsx}"]
      }
    }
  }
}
