{
  "name": "i18n-action",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/i18n-action",
  "projectType": "library",
  "tags": [],
  "targets": {
    "build": {
      "executor": "nx:run-script",
      "options": {
        "script": "build"
      },
      "outputs": ["{workspaceRoot}/actions/i18n"]
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": [
          "packages/i18n-action/**/*.ts",
          "packages/i18n-action/package.json"
        ]
      }
    }
  }
}
