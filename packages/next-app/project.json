{
  "root": "packages/next-app",
  "sourceRoot": "packages/next-app",
  "projectType": "library",
  "tags": [],
  "targets": {
    "build-esm": {
      "executor": "@nrwl/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/packages/next-app",
        "tsConfig": "packages/next-app/tsconfig.lib.json",
        "project": "packages/next-app/package.json",
        "main": "packages/next-app/index.ts",
        "buildableProjectDepsInPackageJsonType": "dependencies"
      }
    },
    "build-web": {
      "executor": "@nrwl/web:rollup",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/packages/next-app",
        "tsConfig": "packages/next-app/tsconfig.lib.json",
        "project": "packages/next-app/package.json",
        "entryFile": "packages/next-app/index.ts",
        "rollupConfig": "@nrwl/react/plugins/bundle-rollup",
        "external": ["react/jsx-runtime"],
        "format": ["umd"],
        "deleteOutputPath": false
      }
    },
    "build": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "commands": ["nx build-esm next-app", "nx build-web next-app"],
        "parallel": false
      }
    },
    "build-clean": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "commands": ["rm -rf dist/packages/next-app/index.js"]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/next-app/**/*.{ts,tsx,js,jsx}"]
      }
    }
  }
}
