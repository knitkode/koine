{
  "npmScope": "koine",
  "affected": {
    "defaultBase": "main"
  },
  "workspaceLayout": {
    "libsDir": "packages"
  },
  "targetDependencies": {
    "build-tsup": [
      {
        "target": "build-tsup",
        "projects": "dependencies"
      }
    ],
    "build-tsc": [
      {
        "target": "build-tsc",
        "projects": "dependencies"
      }
    ],
    "build": [
      {
        "target": "build-tsc",
        "projects": "self"
      },
      {
        "target": "build-tsup",
        "projects": "self"
      }
    ]
  },
  "tasksRunnerOptions": {
    "default": {
      "_runner": "@nrwl/nx-cloud",
      "runner": "nx/tasks-runners/default",
      "options": {
        "cacheableOperations": [
          "build-tsup",
          "build-tsc",
          "lint",
          "test",
          "e2e"
        ],
        "accessToken": "MTU5NzUzZmMtMmMxMS00YTYwLTljZjEtZDAwODNkNGUyZWYxfHJlYWQtd3JpdGU="
      }
    }
  },
  "generators": {
    "@nrwl/react": {
      "application": {
        "babel": true
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/react"
  }
}
