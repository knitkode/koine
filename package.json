    "@mui/base": "^5.0.0-alpha.115",
    "@mui/material": "^5.11.6",
    "@tiptap/react": "^2.0.0-beta.209",
    "@tiptap/starter-kit": "^2.0.0-beta.209",
    "core-js": "^3.27.2",
    "date-fns": "^2.29.3",
    "date-fns-tz": "^1.3.7",
    "framer-motion": "^8.5.5",
    "next": "^13.1.6",
    "next-auth": "^4.19.0",
    "next-seo": "^5.15.0",
    "next-translate": "^1.6.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.43.0",
    "react-icons": "^4.7.1",
    "react-is": "^18",
    "react-popper": "^2.3.0",
    "react-swipeable": "^7.0.0",
    "regenerator-runtime": "^0.13.11",
    "styled-components": "^5.3.6",
    "swr": "^2.0.3",
    "tailwindcss": "^3.2.4",
    "ts-debounce": "^4.0.0",
    "tslib": "^2.5.0",
    "type-fest": "^3.5.3"
  },
  "devDependencies": {
