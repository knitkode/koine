{
  "name": "koine",
  "version": "2.0.0-beta.3",
  "license": "MIT",
  "scripts": {
    "dev": "ts-node tools/dev.ts",
    "start": "nx serve",
    "b": "nx affected --target=build",
    "build": "nx run-many --target=build --all=true",
    "test": "nx run-many --target=test --all=true",
    "lint": "nx run-many --targets=lint --all",
    "_format": "nx format:write",
    "format": "prettier **/*.{js,json,ts,tsx,css,scss,md,yml,yaml,php} --write",
    "story": "npx nx storybook react",
    "clean": "killall node && npx nx reset && rm -rf ./tmp && rm -rf ./dist && rm -rf ./node_modules/.cache",
    "build:story": "npx nx build-storybook react",
    "_lint": "clear && npx eslint --fix packages/**/*.{ts,tsx}",
    "cz": "git-cz",
    "prepare": "husky install",
    "sync": "node ./tools/sync.js",
    "link": "cd ./dist/api && npm link && cd ../browser && npm link && cd ../dom && npm link && cd ../next && npm link && cd ../react && npm link && cd ../utils && npm link",
    "tools": "npx tsc tools/executors/**/**.ts --skipLibCheck true",
    "bt": "npm run tools && npm run clean && clear && npm run build",
    "publish": "cd ./dist/api && npm publish && cd ../browser && npm publish && cd ../dom && npm publish && cd ../next && npm publish && cd ../react && npm publish && cd ../utils && npm publish"
  },
  "private": true,
