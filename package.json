{
  "name": "koine",
  "version": "1.0.79",
  "license": "MIT",
  "scripts": {
    "start": "nx serve",
    "b": "nx affected --target=build",
    "build": "nx run-many --target=build --all=true",
    "test": "nx test",
    "format": "nx format:write",
    "story": "npx nx storybook react",
    "build:story": "npx nx build-storybook react",
    "lint": "clear && npx eslint --fix packages/**/*.{ts,tsx}",
    "clean": "killall node && npx nx reset && rm -rf ./tmp && rm -rf ./dist && rm -rf ./node_modules/.cache",
    "cz": "git-cz",
    "prepare": "husky install",
    "sync": "node ./tools/sync.js",
    "link": "cd ./dist/api && npm link && cd ../next && npm link && cd ../react && npm link && cd ../utils && npm link",
    "tools": "npx tsc tools/executors/build/impl --skipLibCheck true",
    "bt": "npm run tools && npm run clean && clear && npm run build",
    "publish": "cd ./dist/api && npm publish && cd ../next && npm publish && cd ../react && npm publish && cd ../utils && npm publish"
  },
  "private": true,
  "dependencies": {
    "@emotion/react": "^11.10.4",
    "@emotion/server": "^11.10.0",
    "@emotion/styled": "^11.10.4",
    "@hookform/resolvers": "^2.9.7",
    "@kuus/yup": "^1.0.0-beta.4",
    "@mui/base": "^5.0.0-alpha.95",
    "@mui/material": "^5.10.3",
    "@tiptap/react": "^2.0.0-beta.115",
    "@tiptap/starter-kit": "^2.0.0-beta.192",
    "@types/react": "^18.0.18",
    "@types/react-dom": "^18.0.6",
    "@types/styled-components": "^5.1.26",
    "core-js": "^3.25.0",
    "date-fns": "^2.29.2",
    "date-fns-tz": "^1.3.7",
    "framer-motion": "^7.2.1",
    "next": "^12.2.5",
    "next-auth": "^4.10.3",
    "next-seo": "^5.5.0",
    "next-translate": "^1.5.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.34.2",
    "react-icons": "^4.4.0",
    "react-is": "^18",
    "react-popper": "^2.3.0",
    "react-swipeable": "^7.0.0",
    "regenerator-runtime": "^0.13.9",
    "styled-components": "^5.3.5",
    "swr": "^2.0.0-beta.6",
    "tailwindcss": "^3.1.8",
    "tslib": "^2.4.0",
    "type-fest": "^2.19.0"
  },
  "devDependencies": {
    "@babel/core": "7.18.13",
    "@babel/preset-typescript": "7.18.6",
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-angular": "^17.1.0",
    "@commitlint/config-conventional": "^17.1.0",
    "@nrwl/cli": "14.4.3",
    "@nrwl/devkit": "14.4.3",
    "@nrwl/eslint-plugin-nx": "14.4.3",
    "@nrwl/jest": "14.4.3",
    "@nrwl/js": "14.4.3",
    "@nrwl/linter": "14.4.3",
    "@nrwl/next": "14.4.3",
    "@nrwl/react": "14.4.3",
    "@nrwl/storybook": "14.4.3",
    "@nrwl/web": "14.4.3",
    "@nrwl/workspace": "14.4.3",
    "@storybook/addon-essentials": "^6.4.22",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/core-server": "^7.0.0-alpha.6",
    "@storybook/manager-webpack5": "^7.0.0-alpha.6",
    "@storybook/react": "^7.0.0-alpha.6",
    "@svgr/webpack": "^6.3.1",
    "@types/jest": "^29.0.0",
    "@types/node": "^18.7.15",
    "@typescript-eslint/eslint-plugin": "^5.36.1",
    "@typescript-eslint/parser": "^5.36.1",
    "babel-jest": "^29.0.2",
    "babel-loader": "^8.2.5",
    "babel-plugin-styled-components": "^2.0.7",
    "eslint": "^8.23.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-react": "^7.31.6",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.1",
    "jest": "^29.0.2",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "sync-directory": "^5.1.7",
    "typescript": "^4.8.2",
    "webpack": "^5.74.0"
  },
  "workspaces": [
    "packages/**"
  ],
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "packages/**/*.{js,json,css,ts,tsx}": [
      "prettier --write"
    ],
    "packages/**/*.{js,ts,tsx}": [
      "eslint --fix"
    ]
  }
}
