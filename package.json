{
  "name": "koine",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "start": "nx serve",
    "build": "nx run-many --target=build --all=true",
    "test": "nx test",
    "format": "nx format:write",
    "clean": "rm -rf ./tmp && rm -rf ./dist && killall node && rm -rf ./node_modules/.cache/nx",
    "cz": "cz",
    "prepare": "husky install",
    "sync": "node ./tools/sync.js"
  },
  "private": true,
  "workspaces": [
    "packages/**"
  ],
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "packages/**/*.{js,json,css,ts,tsx}": [
      "prettier --write"
    ],
    "packages/**/*.{js,ts,tsx}": [
      "eslint --fix"
    ]
  },
  "devDependencies": {
    "@commitlint/cli": "^16.2.3",
