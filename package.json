    "@nrwl/next": "15.3.3",
    "@nrwl/react": "15.3.3",
    "@nrwl/storybook": "15.3.3",
    "@nrwl/web": "15.3.3",
    "@nrwl/workspace": "15.3.3",
    "@storybook/addon-essentials": "^7.0.0-beta.36",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/core-server": "^7.0.0-beta.36",
    "@storybook/react": "^7.0.0-beta.36",
