    "@nrwl/jest": "13.10.0",
    "@nrwl/linter": "13.10.0",
    "@nrwl/next": "^13.10.0",
    "@nrwl/nx-cloud": "latest",
    "@nrwl/react": "^13.10.0",
    "@nrwl/web": "^13.10.0",
    "@nrwl/workspace": "13.10.0",
    "@swc/cli": "^0.1.57",
