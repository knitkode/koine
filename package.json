{
  "name": "koine",
  "version": "1.0.12",
  "license": "MIT",
  "scripts": {
    "start": "nx serve",
    "b": "nx affected --target=build",
    "build": "nx run-many --target=build --all=true",
    "test": "nx test",
    "format": "nx format:write",
    "story": "npx nx storybook react",
    "build:story": "npx nx build-storybook react",
    "lint": "clear && npx eslint --fix packages/**/*.{ts,tsx}",
    "clean": "killall node && npx nx reset && rm -rf ./tmp && rm -rf ./dist && rm -rf ./node_modules/.cache",
    "cz": "git-cz",
    "prepare": "husky install",
    "sync": "node ./tools/sync.js",
    "link": "cd ./dist/next && npm link && cd ../react && npm link && cd ../utils && npm link",
    "tools": "npx tsc tools/executors/build/impl --skipLibCheck true",
    "bt": "npm run tools && npm run clean && clear && npm run build"
  },
  "private": true,
  "dependencies": {
    "@emotion/react": "^11.9.0",
    "@emotion/server": "^11.4.0",
    "@emotion/styled": "^11.8.1",
    "@hookform/resolvers": "^2.8.10",
    "@mui/base": "^5.0.0-alpha.81",
    "@mui/material": "^5.8.0",
    "@tiptap/react": "^2.0.0-beta.109",
    "@tiptap/starter-kit": "^2.0.0-beta.184",
    "@types/react": "^16.8 || ^17 || ^18",
    "@types/react-dom": "^16.8 || ^17 || ^18",
    "@types/styled-components": "^5.1.25",
    "@types/yup": "^0.29.14",
    "core-js": "^3.22.5",
    "date-fns": "^2.28.0",
    "framer-motion": "^6.3.3",
    "next": "^12.1.6",
    "next-auth": "^4.3.4",
    "next-seo": "^5.4.0",
    "next-translate": "^1.4.0",
    "react": "^16.8 || ^17 || ^18",
    "react-dom": "^16.8 || ^17 || ^18",
    "react-hook-form": "^7.31.1",
    "react-icons": "^4.3.1",
    "react-is": "^16.8 || ^17 || ^18",
    "react-swipeable": "^7.0.0",
    "regenerator-runtime": "^0.13.9",
    "styled-components": "^5.3.5",
    "tailwindcss": "^3.0.24",
    "tslib": "^2.4.0",
    "type-fest": "^2.12.2",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@babel/core": "7.17.12",
    "@babel/preset-typescript": "7.17.12",
