    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.5.0",
    "husky": "^8.0.1",
    "jest": "^28.1.0",
    "lint-staged": "^12.4.1",
    "prettier": "^2.6.2",
    "sync-directory": "^5.0.8",
    "ts-jest": "^28.0.2",
    "typescript": "^4.6.4",
    "webpack": "^5.72.1"
  },
  "workspaces": [
    "packages/**"
  ],
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "packages/**/*.{js,json,css,ts,tsx}": [
      "prettier --write"
    ],
    "packages/**/*.{js,ts,tsx}": [
      "eslint --fix"
    ]
  }
